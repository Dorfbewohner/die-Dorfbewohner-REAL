import time
import webbrowser
print ("Alle 90 Minuten gibt es eine Pause, ran an die Arbeit!")

counter=0

while counter <4:
    time.sleep(1)
    print("90 Minuten sind Rum. Kleine Pause für dich!")
    time.sleep(2)
    webbrowser.open("https://www.youtube.com/watch?v=dQw4w9WgXcQ")
    time.sleep(1)
    webbrowser.open("https://www.youtube.com/watch?v=daQw4w9WgXcQ")
    time.sleep(1)
    print("deine 7,5 Minütige Pause ist rum, ran an die arbeit!")
    counter+=1

time.sleep(1)
print("Feierabend mein Guter.")
