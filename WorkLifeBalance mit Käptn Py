import time
import webbrowser
import random
from random import randint
number=randint(1,100)
tries=6


         
time.sleep(5400)
print("90 Minuten sind Rum. Kleine Pause für dich!")
time.sleep(2)
webbrowser.open("https://www.youtube.com/watch?v=dQw4w9WgXcQ")
time.sleep(199)
webbrowser.open("https://www.youtube.com/watch?v=dQw4w9WgXcQ")
time.sleep(251)

warm=input("Willst du ein Warm-Up?")
if warm.lower()=="ja":

    while tries>0:
        print ("Du hast noch",tries,"Versuche")
        guess=input("Wähle eine Zahl zwischen 1-100\n")
        if int(guess)<number:
            print("Zu niedrig du Holzbein!\n")
            tries-=1
        elif int(guess)>100:
            print("ich sagte 1-100 du Landratte!\n")
            break
        elif int(guess)>number:
            print("zu hoch mein Sohnemann.\n")
            tries-=1
        elif int(guess)==number: 
            print("DU HAST DIE BEUTE ERLANGT!!! ARRRGGGGGHHH")
            break
    if tries ==0:
        print("Du hast verloren du Jammerlappen, Versuch es nochmal!")
else:
    print("okay, kein Warm-Up!")
print("deine 7,5 Minütige Pause ist rum, ran an die arbeit!")
time.sleep(5400)
